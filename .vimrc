execute pathogen#infect()
syntax enable
colorscheme solarized
filetype plugin indent on
set exrc
set secure
set background=dark
set number
set wildmenu
set shiftwidth=4
set softtabstop=4
set tabstop=4
set noexpandtab
set autoindent
set smartindent

let g:ycm_path_to_python_interpreter = '/usr/bin/python'

nnoremap <F5> :NERDTree
nnoremap <F6> :NERDTreeClose
